var height = 175.5;  // å·»ãä¸Šã’ã®å¯¾è±¡

// å¤‰æ•°å®£è¨€ï¼ˆlet - æ¨å¥¨ï¼‰
let name2 = "éˆ´æœ¨èŠ±å­";  // ãƒ–ãƒ­ãƒƒã‚¯ã‚¹ã‚³ãƒ¼ãƒ—
let age2 = 23;  // å†ä»£å…¥å¯èƒ½
let height2 = 162.0;  // å·»ãä¸Šã’ã•ã‚Œãªã„

// å®šæ•°å®£è¨€ï¼ˆconst - æ¨å¥¨ï¼‰
